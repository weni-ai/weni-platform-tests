{
    "type": "object",
    "required": ["uuid", "contact", "queue", "created_on", "modified_on", "is_active", "custom_fields", "callback_url"],
    "properties": {
        "uuid": { "type": "number" },
        "user": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": ["first_name", "last_name", "email"],
                "properties": {
                    "first_name": { "type": "string" },
                    "last_name": { "type": "string" },
                    "email": { "type": "string" }
                }
            }
        },
        "contact": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": ["external_id", "name", "email", "status", "phone", "custom_fields", "created_on"],
                "properties": {
                    "external_id": { "type": "string" },
                    "name": { "type": "string" },
                    "email": { "type": "string" },
                    "status": { "type": "string" },
                    "phone": { "type": "string" },
                    "custom_fields": { "type": "string" },
                    "created_on": { "type": "string" }
                }
            }
        },
        "queue": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": ["uuid", "created_on", "modified_on", "name", "sector"],
                "properties": {
                    "uuid": { "type": "string" },
                    "created_on": { "type": "string" },
                    "modified_on": { "type": "string" },
                    "name": { "type": "string" },
                    "sector": { "type": "string" }
                }
            }
        },
        "created_on": { "type": "string" },
        "modified_on": { "type": "string" },
        "is_active": {"type": "boolean"},
        "custom_fields": {"type": "object"},
        "callback_url": { "type": "string" }
    }
}
